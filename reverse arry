#include<stdio.h>
#include<conio.h>
int main()
{
 int arr[10],i;
 printf("any 10 elememts:"); 
 for(i=0; i<10; i++) 
 {scanf("%d",&arr[i]);
 }
 printf("\nthe arry in reverse order:\n");
for(i=9;i>=0;i--)
{
if(i==0)
printf("%d",arr[i]);

else
printf("%d",arr[i]);
}

getch();

return 0;
}


